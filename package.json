{
  "name": "storefront_backend",
  "version": "1.0.0",
  "description": "This repo contains a basic Node and Express app to get you started in constructing an API. To get started, clone this repo and run `yarn` in your terminal at the project root.",
  "main": "server.ts",
  "scripts": {
    "build": "npx tsc",
    "test": "npm run build && set ENV=test && db-migrate db:create shopping_test && db-migrate --env test up && jasmine-ts && db-migrate db:drop shopping_test",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess 'node ./build/server.js'",
    "test1": "npm run build && set ENV=test && db-migrate db:create shopping_test && db-migrate --env test up && jasmine-ts",
    "test2": "db-migrate db:drop shopping_test",
    "tsc": "tsc",
    "migrate": "db-migrate up ",
    "migrate_down": "db-migrate down -c 5 ",
    "start": "node build/server.js",
    "dev-server": "tsc-watch --noClear -p ./src/tsconfig.json --onSuccess \"node ./build/server.js\"",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "lint": "eslint . --ext .ts"
  },
  "author": "Elena Kochepasova",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/pg": "^8.6.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "prettier": "^2.4.1",
    "save-dev": "0.0.1-security",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jasmine": "^3.10.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "db-migrate": "^1.0.0-beta.16",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.1.0",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "ts-node": "^10.3.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.4"
  }
}
